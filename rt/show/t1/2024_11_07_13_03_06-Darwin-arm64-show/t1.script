cmdResultFn="/Users/mcawood/Lmod/Lmod/rt/show/t1/2024_11_07_13_03_06-Darwin-arm64-show/results.lua"; export cmdResultFn

     . /Users/mcawood/Lmod/Lmod/rt/common_funcs.sh
     unsetMT
     initStdEnvVars
     MODULEPATH_ROOT=/Users/mcawood/Lmod/Lmod/rt/show/mf;  export MODULEPATH_ROOT
     MODULEPATH=$MODULEPATH_ROOT/Core #:$MODULEPATH_ROOT/Compiler/intel/10.1;
