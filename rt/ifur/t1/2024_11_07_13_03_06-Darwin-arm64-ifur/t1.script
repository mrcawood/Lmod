runtimeFn="/Users/mcawood/Lmod/Lmod/rt/ifur/t1/2024_11_07_13_03_06-Darwin-arm64-ifur/t1.runtime"; export runtimeFn
resultFn="/Users/mcawood/Lmod/Lmod/rt/ifur/t1/2024_11_07_13_03_06-Darwin-arm64-ifur/t1.result"; export resultFn
