cmdResultFn="/Users/mcawood/Lmod/Lmod/rt/ifur/t1/2024_11_07_13_03_06-Darwin-arm64-ifur/results.lua"; export cmdResultFn

     . /Users/mcawood/Lmod/Lmod/rt/common_funcs.sh

     unsetMT
     initStdEnvVars
     export MODULEPATH_ROOT=/Users/mcawood/Lmod/Lmod/rt/ifur/mf
     export MODULEPATH=$MODULEPATH_ROOT/core

     remove_generated_lmod_files

     cp /Users/mcawood/Lmod/Lmod/init/lmodrc.lua .lmodrc.lua

     runLmod --version                # 1
     runLmod load stdenv              # 2
