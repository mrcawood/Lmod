     runLmod list                                #13
     runLmod load PrgEnv openmpi                 #14
     runLmod show tclpurge                       #15
     runLmod load tclpurge                       #16
     runLmod list                                #17

     

     HOME=$ORIG_HOME
     PATH=$ORIGINAL_PATH

     cat _stdout.[0-9][0-9][0-9] > _stdout.orig
     joinBase64Results  -bash  _stdout.orig _stdout.new
     cleanUp _stdout.new out.txt

     cat _stderr.[0-9][0-9][0-9] > _stderr.orig
     cleanUp _stderr.orig err.txt

     rm -f results.csv
     wrapperDiff --csv results.csv /Users/mcawood/Lmod/Lmod/rt/purge/out.txt out.txt
     wrapperDiff --csv results.csv /Users/mcawood/Lmod/Lmod/rt/purge/err.txt err.txt
     testFinish -r /Users/mcawood/Lmod/Lmod/rt/purge/t1/2024_11_07_13_03_06-Darwin-arm64-purge/t1.result -t /Users/mcawood/Lmod/Lmod/rt/purge/t1/2024_11_07_13_03_06-Darwin-arm64-purge/t1.runtime results.csv
   