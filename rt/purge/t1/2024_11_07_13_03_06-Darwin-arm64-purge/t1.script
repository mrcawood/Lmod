TARGET=""; export TARGET
runtimeFn="/Users/mcawood/Lmod/Lmod/rt/purge/t1/2024_11_07_13_03_06-Darwin-arm64-purge/t1.runtime"; export runtimeFn
resultFn="/Users/mcawood/Lmod/Lmod/rt/purge/t1/2024_11_07_13_03_06-Darwin-arm64-purge/t1.result"; export resultFn
packageDir="/Users/mcawood/Lmod/Lmod/rt"; export packageDir
testDir="/Users/mcawood/Lmod/Lmod/rt/purge"; export testDir
testName="purge"; export testName
