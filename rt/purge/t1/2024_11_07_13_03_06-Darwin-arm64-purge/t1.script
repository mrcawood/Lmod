outputDir="/Users/mcawood/Lmod/Lmod/rt/purge/t1/2024_11_07_13_03_06-Darwin-arm64-purge"; export outputDir
cmdResultFn="/Users/mcawood/Lmod/Lmod/rt/purge/t1/2024_11_07_13_03_06-Darwin-arm64-purge/results.lua"; export cmdResultFn

     . /Users/mcawood/Lmod/Lmod/rt/common_funcs.sh

     unsetMT
     initStdEnvVars
     LUA=`findcmd lua`
     ORIGINAL_PATH=$PATH
     MODULEPATH_ROOT=/Users/mcawood/Lmod/Lmod/rt/purge/mf;     export MODULEPATH_ROOT
     MODULEPATH=$MODULEPATH_ROOT/Core;  export MODULEPATH

     remove_generated_lmod_files

     cp /Users/mcawood/Lmod/Lmod/init/lmodrc.lua .lmodrc.lua

     runLmod --version                           # 1
     runLmod load PrgEnv openmpi                 # 2
     runLmod list                                # 3
     runLmod unload PrgEnv openmpi               # 4
     runLmod list                                # 5
     runLmod load admin PrgEnv openmpi parmetis  # 6
     runLmod list                                # 7
     runLmod purge                               # 8
     runLmod list                                # 9
     runLmod load PrgEnv openmpi                 #10
     runLmod show test                           #11
