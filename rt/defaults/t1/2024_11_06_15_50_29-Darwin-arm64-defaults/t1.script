     cat _stderr.[0-9][0-9][0-9] > _stderr.orig
     cleanUp _stderr.orig err.txt

     rm -f results.csv
     wrapperDiff --csv results.csv /Users/mcawood/Lmod/Lmod/rt/defaults/out.txt out.txt
     wrapperDiff --csv results.csv /Users/mcawood/Lmod/Lmod/rt/defaults/err.txt err.txt
     testFinish -r /Users/mcawood/Lmod/Lmod/rt/defaults/t1/2024_11_06_15_50_29-Darwin-arm64-defaults/t1.result -t /Users/mcawood/Lmod/Lmod/rt/defaults/t1/2024_11_06_15_50_29-Darwin-arm64-defaults/t1.runtime results.csv
   