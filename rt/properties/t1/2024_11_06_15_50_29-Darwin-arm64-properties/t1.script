     buildSpiderT   $MODULEPATH > ~/.cache/lmod/spiderT.lua
     runLmod avail                      # 7


     HOME=$ORIG_HOME



     cat _stdout.[0-9][0-9][0-9] > _stdout.orig
     joinBase64Results  -bash  _stdout.orig _stdout.new
     cleanUp _stdout.new out.txt

     cat _stderr.[0-9][0-9][0-9] > _stderr.orig
     cleanUp _stderr.orig err.txt

     rm -f results.csv
     wrapperDiff --csv results.csv /Users/mcawood/Lmod/Lmod/rt/properties/out.txt out.txt
     wrapperDiff --csv results.csv /Users/mcawood/Lmod/Lmod/rt/properties/err.txt err.txt
     testFinish -r /Users/mcawood/Lmod/Lmod/rt/properties/t1/2024_11_06_15_50_29-Darwin-arm64-properties/t1.result -t /Users/mcawood/Lmod/Lmod/rt/properties/t1/2024_11_06_15_50_29-Darwin-arm64-properties/t1.runtime results.csv
   