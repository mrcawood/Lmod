     . /Users/mcawood/Lmod/Lmod/rt/common_funcs.sh

     unsetMT
     initStdEnvVars
     MODULEPATH_ROOT=/Users/mcawood/Lmod/Lmod/rt/properties/mf;     export MODULEPATH_ROOT

     remove_generated_lmod_files

     cp /Users/mcawood/Lmod/Lmod/init/lmodrc.lua .lmodrc.lua
     mkdir -p .cache/lmod

     runLmod --version                  # 1
     runLmod use -a /Users/mcawood/Lmod/Lmod/rt/properties/mf/Core  # 2
     runLmod use -a /Users/mcawood/Lmod/Lmod/rt/properties/mf/bio   # 3
