     rm -f results.csv
     wrapperDiff --csv results.csv /Users/mcawood/Lmod/Lmod/rt/genesis/out.txt out.txt
     wrapperDiff --csv results.csv /Users/mcawood/Lmod/Lmod/rt/genesis/err.txt err.txt
     testFinish -r /Users/mcawood/Lmod/Lmod/rt/genesis/t1/2024_11_07_13_03_06-Darwin-arm64-genesis/t1.result -t /Users/mcawood/Lmod/Lmod/rt/genesis/t1/2024_11_07_13_03_06-Darwin-arm64-genesis/t1.runtime results.csv
   